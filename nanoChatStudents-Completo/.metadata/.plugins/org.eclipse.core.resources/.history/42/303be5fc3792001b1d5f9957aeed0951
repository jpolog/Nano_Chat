package es.um.redes.nanoChat.messageML;

public class DuplicatedNick extends NCMessage {

	@Override
	protected String toEncodedString() {
		final char IZQ = '<';
		final char DER = '>';
		final char FIN = '/';
		StringBuffer sb = new StringBuffer();
		String msg = "message";
		String op = "operation";
		char valOp = '3';
		
		sb.append(IZQ+msg+DER);
		sb.append(IZQ+op+DER+valOp+IZQ+FIN+op+DER);
		sb.append(IZQ+FIN+msg+DER);
		String message = sb.toString();
		return message;
	}

}
